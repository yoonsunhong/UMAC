	sql_ven_day_state.append("                ,       SUM(PUR_SAMT) AS PUR_SAMT ");
	sql_ven_day_state.append("                ,       SUM(BOT_SPRC) AS BOT_SPRC ");
	sql_ven_day_state.append("                ,       SUM(BOT_SUM) AS BOT_SUM ");
	sql_ven_day_state.append("                FROM ( ");
	sql_ven_day_state.append("                                SELECT  CSP.VEN_NAME ");
	sql_ven_day_state.append("                                ,       PPH.VEN_CODE ");
	sql_ven_day_state.append("                                ,       CS.STR_NAME ");
	sql_ven_day_state.append("                                ,       PPH.PUR_CFM_DT AS PUR_DT ");
	sql_ven_day_state.append("                                ,       PPD.ITM_CODE ");
	sql_ven_day_state.append("                                ,       PPD.LINK_CODE ");
	sql_ven_day_state.append("                                ,       CPC.ITM_NAME ");
	sql_ven_day_state.append("                                ,       PPD.TAX_GB ");
	sql_ven_day_state.append("                                ,       CASE WHEN PPH.PUR_GB = '2' THEN ROUND(PPD.DEC_QTY) * (-1) ELSE ROUND(PPD.DEC_QTY) END AS DEC_QTY ");
	sql_ven_day_state.append("                                ,       PPD.PUR_WPRC AS PUR_WPRC ");
	sql_ven_day_state.append("                                ,       PPD.PUR_WVAT AS PUR_WVAT ");
