	sql_ven_day_state.append("                                        ELSE DECODE(PPD.TAX_GB, '1', TRUNC(PPD.DEC_QTY * -1 * (PPD.PUR_WPRC + PPD.PUR_WVAT)) ");
	sql_ven_day_state.append("                                                                   - ROUND(PPD.DEC_QTY * -1 * (PPD.PUR_WPRC + PPD.PUR_WVAT)/11, 2) ");
