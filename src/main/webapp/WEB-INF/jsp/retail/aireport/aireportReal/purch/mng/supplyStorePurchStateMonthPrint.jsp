	sql_ven_day_state.append("                                                                   , ROUND(PPD.DEC_QTY * -1 * PPD.PUR_WVAT, 1) ) ");
	sql_ven_day_state.append("                                        END  AS PUR_WVAT2 ");
	sql_ven_day_state.append("                                , CASE WHEN PPH.PUR_GB = '1' THEN ");
	sql_ven_day_state.append("                                            DECODE(PPD.TAX_GB, '1', TRUNC(PPD.DEC_QTY * (PPD.PUR_WPRC + PPD.PUR_WVAT)) ");
	sql_ven_day_state.append("                                                                  , TRUNC(PPD.DEC_QTY * PPD.PUR_WPRC)) ");
	sql_ven_day_state.append("                                        ELSE DECODE(PPD.TAX_GB, '1', TRUNC(PPD.DEC_QTY * -1 * (PPD.PUR_WPRC + PPD.PUR_WVAT)) ");
	sql_ven_day_state.append("                                                                   , TRUNC(PPD.DEC_QTY * -1 * PPD.PUR_WPRC)) ");
	sql_ven_day_state.append("                                        END  AS  PUR_WAMT2 ");
	sql_ven_day_state.append("                                ,       ROUND(PPD.PUR_SPRC) AS PUR_SPRC ");
	sql_ven_day_state.append("                                ,       CASE WHEN PPH.PUR_GB = '2' THEN ROUND(PPD.PUR_SAMT) * (-1) ELSE ROUND(PPD.PUR_SAMT) END AS PUR_SAMT ");
	sql_ven_day_state.append("                                ,       PPH.ROUTE_GB ");
	sql_ven_day_state.append("                                ,       ROUND(PPD.BOT_SPRC) AS BOT_SPRC ");
	sql_ven_day_state.append("                                ,       CASE WHEN PPH.PUR_GB = '2' THEN ROUND(PPD.BOT_SPRC * PPD.DEC_QTY) * (-1) ELSE ROUND(PPD.BOT_SPRC * PPD.DEC_QTY) END AS BOT_SUM ");
	sql_ven_day_state.append("                                ,       PPH.CFM_DT ");
	sql_ven_day_state.append("                                ,       PPD.SLIP_NO ");
	sql_ven_day_state.append("                                FROM    PO_PURCH_HDR PPH ");
	sql_ven_day_state.append("                                JOIN    PO_PURCH_DTL PPD ");
	sql_ven_day_state.append("                                ON      PPH.CORP_CODE = PPD.CORP_CODE ");
	sql_ven_day_state.append("                                AND     PPH.PUR_DT    = PPD.PUR_DT ");
	sql_ven_day_state.append("                                AND     PPH.STR_CODE  = PPD.STR_CODE ");
	sql_ven_day_state.append("                                AND     PPH.SLIP_NO   = PPD.SLIP_NO ");
	sql_ven_day_state.append("                                LEFT JOIN CD_SUPPLY CSP ");
	sql_ven_day_state.append("                                ON      PPH.CORP_CODE = CSP.CORP_CODE ");
	sql_ven_day_state.append("                                AND     PPH.VEN_CODE  = CSP.VEN_CODE ");
	sql_ven_day_state.append("                                AND     CSP.GRE_GB IN ('1','2') ");
	sql_ven_day_state.append("                                LEFT JOIN CD_STORE CS ");
	sql_ven_day_state.append("                                ON      PPH.CORP_CODE = CS.CORP_CODE ");
	sql_ven_day_state.append("                                AND     PPH.STR_CODE  = CS.STR_CODE ");
	sql_ven_day_state.append("                                LEFT JOIN CD_PRODUCT_CMN CPC ");
	sql_ven_day_state.append("                                ON      PPH.CORP_CODE = CPC.CORP_CODE ");
	sql_ven_day_state.append("                                AND     PPD.ITM_CODE  = CPC.ITM_CODE ");
	sql_ven_day_state.append("                                AND     PPD.LINK_CODE = CPC.SCAN_CODE ");
	sql_ven_day_state.append("                                WHERE   PPH.CORP_CODE = '^P_CORP_CODE^' ");
	sql_ven_day_state.append("                                AND     PPH.PUR_CFM_DT BETWEEN REPLACE('^P_SEARCH_START_DT^', '-') AND REPLACE('^P_SEARCH_END_DT^', '-') ");
	sql_ven_day_state.append("                                AND     PPH.VEN_CODE  = NVL('^P_VEN_CODE^', PPH.VEN_CODE) ");
	sql_ven_day_state.append("                                AND     PPH.STR_CODE  = NVL('^P_STR_CODE^', PPH.STR_CODE) ");
	sql_ven_day_state.append("                                AND     PPH.PUR_GB    = NVL('^P_PUR_GB^', PPH.PUR_GB) ");
	sql_ven_day_state.append("                                AND     CSP.GRE_GB    = NVL('^P_GRE_GB^',CSP.GRE_GB) ");
	sql_ven_day_state.append("                                AND     PPH.PUR_GB    IN ('1','2') ");
	sql_ven_day_state.append("                                AND     PPD.ORD_FLAG   IN ('1','2') ");
	sql_ven_day_state.append("                                AND     PPH.CFM_DT    IS NOT NULL ");
	sql_ven_day_state.append("                ) A ");
	sql_ven_day_state.append("                GROUP BY VEN_NAME ");
	sql_ven_day_state.append("                ,       VEN_CODE ");
	sql_ven_day_state.append("                ,       STR_NAME ");
	sql_ven_day_state.append("                ,       TAX_GB      )   AA ");
	sql_ven_day_state.append(" ");
	sql_ven_day_state.append("                GROUP BY    AA.VEN_NAME ");
	sql_ven_day_state.append("                ,           AA.VEN_CODE ");
	sql_ven_day_state.append("                ,           AA.STR_NAME) AAA ");
	sql_ven_day_state.append("                ORDER BY    AAA.VEN_CODE ");
	initializer.prepareQuery("sql_ven_day_state", sql_ven_day_state.toString());
}

