		sheet20_A2_Buf=null;
		sheet20_A2_Flag=true;
		sheet20_A2="";
		sheet20_B2="";
		sheet20_C2="";
		sheet20_D2="";
		sheet20_E2="";
		sheet20_F2="";
		sheet20_G2="";
		sheet20_H2="";
		sheet20_I2=0;
		bEnabledVirtualMerge_fv=false;

		//DataSet 객체 생성
		//ResultSet credit_receivables 객체의 생성
		if(credit_receivables==null){
			credit_receivables = stat_credit_receivables.executeQuery();
		}else{
			credit_receivables.beforeFirst();
		}

		//--while Loop Start--##
		while(credit_receivables.next()){   //자동으로 생성되는 반복 밴드의 루프문
			//데이터 fetch
			setFieldVariableFromcredit_receivables();	//RecordSet credit_receivables에서 필드 값 설정

			//조건 밴드 출력 조건문
			bIsBeforePageSkip=false;
			if(!credit_receivables_STR_NAME.equals(sheet20_A2)){    //fv_1 밴드 조건문
				bIsTrue_fv_1=true;
			}else{
				bIsTrue_fv_1=false;
			}

			if(bIsFirst){   //밴드 호출 후 루프 내에서 한번 만 실행되는 코드
				//조건 플래그 초기화
				bIsTrue_fv_1=false;    //fv_1 조건 밴드 플래그 초기화
				bIsTrue_fv_2=false;    //fv_2 조건 밴드 플래그 초기화
			}   //end if(bIsFirst)

			//데이터 변수 할당
			if(!sheet20_A2.equals(credit_receivables_STR_NAME)){
				if(!bIsFirst){
					sheet20_A2_Buf = sheet20_A2;
					sheet20_A2_Flag=true;
				}
				sheet20_A2=credit_receivables_STR_NAME;
			}
			sheet20_B2 = credit_receivables_CUST_NO;
			sheet20_C2 = credit_receivables_CUST_NAME;
			sheet20_D2 = credit_receivables_MBR_GRADE_NAME;
			sheet20_E2 = credit_receivables_BUSI_FLAG_NAME;
			sheet20_F2 = credit_receivables_ACCT_DEPT;
			sheet20_G2 = credit_receivables_SALE_DT;
			sheet20_H2 = credit_receivables_RCP_DT;
			sheet20_I2 = (double)credit_receivables_ACCT_REABLE;

			//루프 내부 가상병합 출력 함수 호출
			if(bIsPrint){
				writeScriptVirtualMergefv();
			}

			//루프 내부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_1(bIsFirst, false, bIsTrue_fv_1, true);   //fv_1 조건 밴드 호출
			getScriptFromfv_2(bIsFirst, false, bIsTrue_fv_2, true);   //fv_2 조건 밴드 호출

			//AI 스크립트 출력
			if(bIsPrint){   //감추기 속성이 지정되지 않으면 bIsPrint는 true
				if(isPageSkip(nYCurPos+nBandHeight)){   //스크립트 출력 영역 검사
					//가상병합 출력 및 한번만 데이터 Reset
					pageSkipVirtualMergefv();

					//페이지 스킵 함수 호출
					pageSkip(10);
				}   //end if(page skip test)

				//스크립트 출력 함수 호출
				writeScriptfv();

				//페이지 연결 출력 및 가상병합 플래그 Reset
				if(sheet20_A2_Flag){
					sheet20_A2_YPos=nYCurPos;
					sheet20_A2_Buf=null;
					sheet20_A2_Flag=false;
				}
				bEnabledVirtualMerge_fv=true;

				//다음 출력 좌표 설정
				nYCurPos=nYCurPos+nBandHeight;
			}   //end if(bIsPrint)

			//bIsFirst 및 출력 라인 설정
			bIsFirst=false;
		}   //while end

		if(bIsFirst){   //while 루프가 실행되지 않은 경우
		} else {   //while 루프가 실행된 경우
			//루프 외부 가상병합 출력 함수 호출
			sheet20_A2_Buf = sheet20_A2;
			if(bIsPrint){
				writeScriptVirtualMergefv();
			}

			//루프 외부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_1(bIsFirst, true, true, true);   //fv_1 조건 밴드 호출
			getScriptFromfv_2(bIsFirst, true, true, true);   //fv_2 조건 밴드 호출
		}   //루프 외부 end if(bIsFirst)
	}

