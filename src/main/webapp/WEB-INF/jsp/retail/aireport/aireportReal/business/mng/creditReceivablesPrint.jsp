	private void getScriptFromfv(boolean bIsPrint) throws Exception{
		//지역 변수 선언 및 초기화
		int nBandHeight=17;    //밴드 출력 높이
		boolean bIsFirst=true;
		boolean bIsTrue_fv_1=false;   //fv_1 조건 밴드 플래그 선언
		boolean bIsTrue_fv_2=false;   //fv_2 조건 밴드 플래그 선언

