	private void getScriptFromfv1(boolean bIsPrint) throws Exception{
		//지역 변수 선언 및 초기화
		int nBandHeight=15;    //밴드 출력 높이
		boolean bIsFirst=true;
		boolean bIsTrue_fv_sub=false;   //fv_sub 조건 밴드 플래그 선언
		boolean bIsTrue_fv_1=false;   //fv_1 조건 밴드 플래그 선언

		//데이터 변수 초기화
		sheet30_A2_Buf=null;
		sheet30_A2_Flag=true;
		sheet30_A2="";
		sheet30_B2_Buf=null;
		sheet30_B2_Flag=true;
		sheet30_B2="";
		sheet30_C2_Buf=null;
		sheet30_C2_Flag=true;
		sheet30_C2="";
		sheet30_D2="";
		sheet30_E2="";
		sheet30_F2="";
		sheet30_G2="";
		sheet30_H2="";
		sheet30_I2="";
		sheet30_J2=0L;
		sheet30_K2=0L;
		sheet30_L2=0L;
		sheet30_M2=0L;
		bEnabledVirtualMerge_fv1=false;

		//DataSet 객체 생성
		//ResultSet SALE_LIST 객체의 생성
		if(SALE_LIST==null){
			SALE_LIST = stat_SALE_LIST.executeQuery();
		}else{
			SALE_LIST.beforeFirst();
		}

		//반복 헤더 호출
		getScriptFromfv1_h(bIsFirst, true);	//반복 헤더 호출

		//--while Loop Start--##
		while(SALE_LIST.next()){   //자동으로 생성되는 반복 밴드의 루프문
			//데이터 fetch
			setFieldVariableFromSALE_LIST();	//RecordSet SALE_LIST에서 필드 값 설정

			//조건 밴드 출력 조건문
			bIsBeforePageSkip=false;
			if(!SALE_LIST_SLIP_NO.equals(sheet30_C2)){    //fv_sub 밴드 조건문
				bIsTrue_fv_sub=true;
			}else{
				bIsTrue_fv_sub=false;
			}

			if(bIsFirst){   //밴드 호출 후 루프 내에서 한번 만 실행되는 코드
				//조건 플래그 초기화
