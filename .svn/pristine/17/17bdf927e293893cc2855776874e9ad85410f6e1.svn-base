public void jspInit() {
	initializer = new ReportInitializer(ResultSet.TYPE_SCROLL_INSENSITIVE, ResultSet.CONCUR_READ_ONLY);
	//수정되지 않는 정적 Query변수들을 초기화
	//sql_SALE_LIST
	StringBuilder sql_SALE_LIST = new StringBuilder();
	sql_SALE_LIST.setLength(0);
	sql_SALE_LIST.append("SELECT ");
	sql_SALE_LIST.append("        ROWNUM ");
	sql_SALE_LIST.append("       ,PUH.CORP_CODE  AS CORP_CODE ");
	sql_SALE_LIST.append("      , SUBSTR(PUH.PUR_DT,0,4)||'-'|| ");
	sql_SALE_LIST.append("        SUBSTR(PUH.PUR_DT,5,2)||'-'|| ");
	sql_SALE_LIST.append("        SUBSTR(PUH.PUR_DT,7,2)    AS PUR_DT ");
	sql_SALE_LIST.append("      , SUBSTR(PUH.PUR_CFM_DT,0,4)||'-'|| ");
	sql_SALE_LIST.append("        SUBSTR(PUH.PUR_CFM_DT,5,2)||'-'|| ");
	sql_SALE_LIST.append("        SUBSTR(PUH.PUR_CFM_DT,7,2)    AS PUR_CFM_DT ");
	sql_SALE_LIST.append("      , PUH.STR_CODE   AS STR_CODE ");
	sql_SALE_LIST.append("      , PUH.SLIP_NO    AS SLIP_NO ");
	sql_SALE_LIST.append("      , FN_GET_COMMON_CODE_NM('PUR_GB',PUH.PUR_GB,PUH.CORP_CODE) AS PUR_GB ");
	sql_SALE_LIST.append("      , PUH.ROUTE_GB   AS ROUTE_GB ");
	sql_SALE_LIST.append("      , PUH.VEN_CODE   AS VEN_CODE ");
	sql_SALE_LIST.append("      , (SELECT CDS.VEN_NAME FROM CD_SUPPLY CDS WHERE CDS.CORP_CODE = PUH.CORP_CODE AND CDS.VEN_CODE = PUH.VEN_CODE) AS VEN_NAME ");
	sql_SALE_LIST.append("      , PUD.ITM_CODE   AS ITM_CODE ");
	sql_SALE_LIST.append("      , FN_GET_ITEM_NM(PUD.ITM_CODE, PUH.CORP_CODE)  AS ITM_NAME ");
	sql_SALE_LIST.append("      , CPS.SCAN_CODE  AS SCAN_CODE ");
	sql_SALE_LIST.append("      , CPS.CLS_CODE   AS CLS_CODE ");
	sql_SALE_LIST.append("      , PUD.UNIT       AS UNIT ");
	sql_SALE_LIST.append("      , PUD.ORD_FLAG   AS ORD_FLAG ");
	sql_SALE_LIST.append("      , FN_GET_COMMON_CODE_NM('ORD_FLAG', PUD.ORD_FLAG, PUH.CORP_CODE) AS ORD_FLAG_NM ");
	sql_SALE_LIST.append("      , PUD.TAX_GB     AS TAX_GB ");
	sql_SALE_LIST.append("      , FN_GET_COMMON_CODE_NM('TAX_GB', PUD.TAX_GB, PUH.CORP_CODE) AS TAX_GB_NM ");
	sql_SALE_LIST.append("      , PUD.ORD_QTY    AS ORD_QTY ");
	sql_SALE_LIST.append("      , PUD.PUR_QTY    AS PUR_QTY ");
	sql_SALE_LIST.append("      , PUD.DEC_QTY   * DECODE(PUH.PUR_GB,'2',-1,1) AS DEC_QTY ");
	sql_SALE_LIST.append("      , PUD.PUR_WPRC   AS PUR_WPRC ");
	sql_SALE_LIST.append("      , PUD.PUR_WVAT   AS PUR_WVAT ");
	sql_SALE_LIST.append("      , ((PUD.PUR_WPRC +PUD.PUR_WVAT) * PUD.DEC_QTY) * DECODE(PUH.PUR_GB,'2',-1,1) AS PUR_WAMT ");
	sql_SALE_LIST.append("      , PUD.PUR_SAMT * DECODE(PUH.PUR_GB,'2',-1,1)  AS PUR_SAMT ");
	sql_SALE_LIST.append("      , (PUD.BOT_SPRC * PUD.DEC_QTY) * DECODE(PUH.PUR_GB,'2',-1,1) AS BOT_SAMT ");
	sql_SALE_LIST.append("      , (PUD.PUR_SAMT - (PUD.PUR_WAMT * PUD.DEC_QTY)) * DECODE(PUH.PUR_GB,'2',-1,1) AS EXPECTED_PROFIT ");
	sql_SALE_LIST.append("      , CASE WHEN PUD.PUR_SAMT = 0 THEN '0.00' ");
	sql_SALE_LIST.append("             ELSE TO_CHAR(((PUD.PUR_SAMT - (PUD.PUR_WAMT * PUD.DEC_QTY)) / PUD.PUR_SAMT) * 100 * DECODE(PUH.PUR_GB,'2',-1,1), '999.99') END ");
	sql_SALE_LIST.append("             AS PROFIT_RATE ");
	sql_SALE_LIST.append("FROM    PO_PURCH_HDR PUH ");
	sql_SALE_LIST.append("      , PO_PURCH_DTL PUD ");
	sql_SALE_LIST.append("      , CD_PRODUCT_STORE CPS ");
	sql_SALE_LIST.append("WHERE PUH.CORP_CODE = PUD.CORP_CODE ");
	sql_SALE_LIST.append("AND   PUH.PUR_DT    = PUD.PUR_DT ");
	sql_SALE_LIST.append("AND   PUH.STR_CODE  = PUD.STR_CODE ");
	sql_SALE_LIST.append("AND   PUH.SLIP_NO   = PUD.SLIP_NO ");
	sql_SALE_LIST.append("AND   PUH.CORP_CODE = CPS.CORP_CODE ");
	sql_SALE_LIST.append("AND   PUH.STR_CODE  = CPS.STR_CODE ");
	sql_SALE_LIST.append("AND   PUD.ITM_CODE  = CPS.ITM_CODE ");
	sql_SALE_LIST.append("AND   PUH.CORP_CODE = '^P_CORP_CODE^' ");
	sql_SALE_LIST.append("AND   PUH.PUR_CFM_DT    BETWEEN '^P_SALE_SD^' AND '^P_SALE_ED^' ");
	sql_SALE_LIST.append("AND   PUH.STR_CODE  = '^P_STR_CODE^' ");
	sql_SALE_LIST.append("AND   PUH.VEN_CODE LIKE '^P_VEN_CODE^'||'%' ");
	sql_SALE_LIST.append("AND   CPS.CLS_CODE LIKE '^P_CLS_CODE^'||'%' ");
	sql_SALE_LIST.append("AND   PUD.ITM_CODE LIKE '^P_ITM_CODE^'||'%' ");
	sql_SALE_LIST.append("AND   PUH.PUR_GB    = NVL('^P_PUR_GB^',PUH.PUR_GB) ");
	sql_SALE_LIST.append("ORDER BY PUH.PUR_DT DESC ");
	initializer.prepareQuery("sql_SALE_LIST", sql_SALE_LIST.toString());

	//sql_TITLE
	StringBuilder sql_TITLE = new StringBuilder();
	sql_TITLE.setLength(0);
	sql_TITLE.append("SELECT ");
	sql_TITLE.append("	TO_CHAR(SYSDATE,'YYYY-MM-DD HH:MI') AS TIME ");
	sql_TITLE.append(",             NVL(FN_GET_ITEM_NM('^P_ITM_CODE^', '^P_CORP_CODE^'),'전체')  AS ITM_NAME ");
	sql_TITLE.append(",	NVL(FN_GET_VEN_NM('^P_VEN_CODE^','^P_CORP_CODE^'),'전체') as VEN_NAME ");
	sql_TITLE.append(",	NVL(FN_GET_STR_NAME('^P_STR_CODE^','^P_CORP_CODE^'),'전체') AS STR_NAME ");
	sql_TITLE.append(",	NVL((SELECT CLS_NAME FROM CD_SML_MST WHERE CLS_CODE='^P_CLS_CODE^'),'전체') AS CLS_NAME ");
	sql_TITLE.append("FROM 	DUAL ");
	sql_TITLE.append(" ");
	initializer.prepareQuery("sql_TITLE", sql_TITLE.toString());
}

