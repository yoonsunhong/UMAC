			if(!sheet30_A2.equals(SALE_LIST_PUR_DT)){
				if(!bIsFirst){
					sheet30_A2_Buf = sheet30_A2;
					sheet30_A2_Flag=true;
				}
				sheet30_A2=SALE_LIST_PUR_DT;
			}
			if(!sheet30_B2.equals(SALE_LIST_PUR_CFM_DT)){
				if(!bIsFirst){
					sheet30_B2_Buf = sheet30_B2;
					sheet30_B2_Flag=true;
				}
				sheet30_B2=SALE_LIST_PUR_CFM_DT;
			}
			if(!sheet30_C2.equals(SALE_LIST_SLIP_NO)){
				if(!bIsFirst){
					sheet30_C2_Buf = sheet30_C2;
					sheet30_C2_Flag=true;
				}
				sheet30_C2=SALE_LIST_SLIP_NO;
			}
			sheet30_D2 = SALE_LIST_PUR_GB;
			sheet30_E2 = SALE_LIST_SCAN_CODE;
			sheet30_F2 = SALE_LIST_ITM_NAME;
			sheet30_G2 = SALE_LIST_UNIT;
			sheet30_H2 = SALE_LIST_ORD_FLAG_NM;
			sheet30_I2 = SALE_LIST_TAX_GB_NM;
			sheet30_J2 = SALE_LIST_DEC_QTY;
			sheet30_K2 = (long)SALE_LIST_PUR_WPRC;
			sheet30_L2 = SALE_LIST_PUR_WVAT;
			sheet30_M2 = SALE_LIST_PUR_WAMT;

			//루프 내부 가상병합 출력 함수 호출
			if(bIsPrint){
				writeScriptVirtualMergefv1();
			}

			//루프 내부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_sub(bIsFirst, false, bIsTrue_fv_sub, true);   //fv_sub 조건 밴드 호출
			getScriptFromfv_1(bIsFirst, false, bIsTrue_fv_1, true);   //fv_1 조건 밴드 호출

			//AI 스크립트 출력
			if(bIsPrint){   //감추기 속성이 지정되지 않으면 bIsPrint는 true
				if(isPageSkip(nYCurPos+nBandHeight)){   //스크립트 출력 영역 검사
					//가상병합 출력 및 한번만 데이터 Reset
					pageSkipVirtualMergefv1();

					//페이지 스킵 함수 호출
					pageSkip(10);

					//반복 헤더 및 부모 반복단위 호출
					getScriptFromfv1_h(false, true);	//반복 헤더 호출
				}   //end if(page skip test)

				//스크립트 출력 함수 호출
				writeScriptfv1();

				//페이지 연결 출력 및 가상병합 플래그 Reset
				if(sheet30_A2_Flag){
					sheet30_A2_YPos=nYCurPos;
					sheet30_A2_Buf=null;
					sheet30_A2_Flag=false;
				}
				if(sheet30_B2_Flag){
					sheet30_B2_YPos=nYCurPos;
					sheet30_B2_Buf=null;
					sheet30_B2_Flag=false;
				}
				if(sheet30_C2_Flag){
					sheet30_C2_YPos=nYCurPos;
					sheet30_C2_Buf=null;
					sheet30_C2_Flag=false;
				}
				bEnabledVirtualMerge_fv1=true;

				//다음 출력 좌표 설정
				nYCurPos=nYCurPos+nBandHeight;
			}   //end if(bIsPrint)

			//bIsFirst 및 출력 라인 설정
			bIsFirst=false;
		}   //while end

		if(bIsFirst){   //while 루프가 실행되지 않은 경우
		} else {   //while 루프가 실행된 경우
			//루프 외부 가상병합 출력 함수 호출
			sheet30_A2_Buf = sheet30_A2;
			sheet30_B2_Buf = sheet30_B2;
			sheet30_C2_Buf = sheet30_C2;
			if(bIsPrint){
				writeScriptVirtualMergefv1();
			}

			//루프 외부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_sub(bIsFirst, true, true, true);   //fv_sub 조건 밴드 호출
			getScriptFromfv_1(bIsFirst, true, true, true);   //fv_1 조건 밴드 호출
		}   //루프 외부 end if(bIsFirst)
	}

	//----fv1 반복밴드 Page Skip시 가상 병합 데이터 설정 및 출력 함수----
	private void pageSkipVirtualMergefv1(  ) throws Exception{
		//가상 병합 데이터 설정 및 출력
		if(bEnabledVirtualMerge_fv1){
			if(sheet30_A2_Buf==null){
				sheet30_A2_Buf = sheet30_A2;
			}else{
				sheet30_A2_Buf=null;
			}
			sheet30_A2_Flag=true;
			if(sheet30_B2_Buf==null){
				sheet30_B2_Buf = sheet30_B2;
			}else{
				sheet30_B2_Buf=null;
			}
			sheet30_B2_Flag=true;
			if(sheet30_C2_Buf==null){
				sheet30_C2_Buf = sheet30_C2;
			}else{
				sheet30_C2_Buf=null;
			}
			sheet30_C2_Flag=true;
			writeScriptVirtualMergefv1();
		}
	}

