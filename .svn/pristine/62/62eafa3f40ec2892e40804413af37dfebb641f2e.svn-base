		sql_CUST_RECEIABLES.append("                              GROUP  BY CUST_NO ");
		sql_CUST_RECEIABLES.append("                            ) TDPOT1 ");
		sql_CUST_RECEIABLES.append(" ");
		sql_CUST_RECEIABLES.append("                    WHERE   CDCL.CORP_CODE  = '^P_CORP_CODE^' ");
		sql_CUST_RECEIABLES.append("                    AND     CDCL.STR_CODE   = '^P_STR_CODE^' ");
		sql_CUST_RECEIABLES.append("                    AND     CDCL.CUST_NO    = NVL('^P_CUST_NO^',CDCL.CUST_NO) ");
		sql_CUST_RECEIABLES.append("                    AND     CC.CORP_CODE    = CDCL.CORP_CODE ");
		sql_CUST_RECEIABLES.append("                    AND     CC.CUST_NO      = CDCL.CUST_NO ");
		sql_CUST_RECEIABLES.append("                    AND     PRE.CUST_NO(+)    = CDCL.CUST_NO ");
		sql_CUST_RECEIABLES.append("                    AND     TSALE.CUST_NO(+)  = CDCL.CUST_NO ");
