	//----fv_1 ConditionBand Start----
	//----소속 쉬트 : SHEET=Sheet3    FRAME=Frame0
	//----밴드 이름 : fv_1
	//----밴드 종류 : 조건 밴드
	//----bIsFrist : 부모 밴드 함수에서 최초로 호출될 때 true
	//----bIsLast : 부모 밴드 함수에서 마지막으로 호출될 때 true
	//----bIsTrue : 부모 밴드 함수에서 조건 밴드 출력 조건 플래그 값
	//----bIsPrint : 감추기 속성 설정 여부. true면 출력, false면 AI 스크립트를 생성하지 않음.
	private void getScriptFromfv_1(boolean bIsFirst, boolean bIsLast, boolean bIsTrue, boolean bIsPrint) throws Exception{
		//지역 변수 선언 및 초기화
		int nBandHeight=20;	//밴드 출력 높이

		if(bIsFirst){   //밴드 최초 호출 시에만 실행되는 코드
			//데이터 변수 초기화
			sheet30_D3=0;
			sheet30_E3=0;
			sheet30_F3=0;
			sheet30_I3=0;
		}   //end if(bIsFirst)

		//AI 스크립트 출력
		if(bIsTrue){   //조건 밴드 출력 플래그 값이 true인 경우
			//조건 밴드의 참 값
			if(bIsPrint){   //WriteBandObj() 함수 대체 코드
				if(isPageSkip(nYCurPos+nBandHeight)){
					pageSkip(10);
					getScriptFromfv1_h(false, true);	//반복 헤더 호출
				}
				writeScriptfv_1();	//스크립트문 출력
				nYCurPos=nYCurPos+nBandHeight;
				sheet30_D3=0;
				sheet30_E3=0;
				sheet30_F3=0;
				sheet30_I3=0;
			}
		}else{   //조건 밴드 출력 플래그 값이 false인 경우
		}   //end if(bIsTrue)

		if(!bIsLast){   //bIsLast가 false인 경우 - 조건 밴드의 수식 계산
			//데이터 변수 할당
			sheet30_A3="합계";
			sheet30_D3=SUM(sheet30_D2, sheet30_D3);
			sheet30_E3=SUM(sheet30_E2, sheet30_E3);
			sheet30_F3=SUM(sheet30_F2, sheet30_F3);
			sheet30_I3=SUM(sheet30_I2, sheet30_I3);
		}   //end if(!bIsLast)
	}

	//-----fv1 RepeatBand Start-----
	//-----소속 쉬트 : SHEET=Sheet3   FRAME=Frame0
	//-----밴드 이름 : fv1
	//-----밴드 종류 : 반복 밴드
	//-----bIsPrint : 감추기 속성 설정 여부 true면 출력, false면 스크립트를 생성하지 않음
	private void getScriptFromfv1(boolean bIsPrint) throws Exception{
		//지역 변수 선언 및 초기화
		int nBandHeight=16;    //밴드 출력 높이
		boolean bIsFirst=true;
		boolean bIsTrue_fv_1=false;   //fv_1 조건 밴드 플래그 선언

		//데이터 변수 초기화
		sheet30_A2="";
		sheet30_B2="";
		sheet30_C2="";
		sheet30_D2=0;
		sheet30_E2=0;
		sheet30_F2=0;
		sheet30_G2="";
		sheet30_H2="";
		sheet30_I2=0;

		//DataSet 객체 생성
		//ResultSet CUST_RECEIABLES 객체의 생성
		if(CUST_RECEIABLES==null){
			CUST_RECEIABLES = stat_CUST_RECEIABLES.executeQuery();
		}else{
			CUST_RECEIABLES.beforeFirst();
		}

		//반복 헤더 호출
		getScriptFromfv1_h(bIsFirst, true);	//반복 헤더 호출

		//--while Loop Start--##
		while(CUST_RECEIABLES.next()){   //자동으로 생성되는 반복 밴드의 루프문
			//데이터 fetch
			setFieldVariableFromCUST_RECEIABLES();	//RecordSet CUST_RECEIABLES에서 필드 값 설정

			if(bIsFirst){   //밴드 호출 후 루프 내에서 한번 만 실행되는 코드
				//조건 플래그 초기화
				bIsTrue_fv_1=false;    //fv_1 조건 밴드 플래그 초기화
			}   //end if(bIsFirst)

			//데이터 변수 할당
			sheet30_A2 = CUST_RECEIABLES_MOBIL_NO;
			sheet30_B2 = CUST_RECEIABLES_CUST_NO;
			sheet30_C2 = CUST_RECEIABLES_CUST_NAME;
			sheet30_D2 = (double)CUST_RECEIABLES_CREDIT_MONTH_AMT;
			sheet30_E2 = (double)CUST_RECEIABLES_CREDIT_SALE;
			sheet30_F2 = (double)CUST_RECEIABLES_DPOT_AMT;
			sheet30_G2 = CUST_RECEIABLES_LAST_SALE_DT_;
			sheet30_H2 = CUST_RECEIABLES_LAST_RCP_DT_;
			sheet30_I2 = (double)CUST_RECEIABLES_ACCT_REABLE;

			//루프 내부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_1(bIsFirst, false, bIsTrue_fv_1, true);   //fv_1 조건 밴드 호출

			//AI 스크립트 출력
			if(bIsPrint){   //감추기 속성이 지정되지 않으면 bIsPrint는 true
				if(isPageSkip(nYCurPos+nBandHeight)){   //스크립트 출력 영역 검사
					//페이지 스킵 함수 호출
					pageSkip(10);

					//반복 헤더 및 부모 반복단위 호출
					getScriptFromfv1_h(false, true);	//반복 헤더 호출
				}   //end if(page skip test)

				//스크립트 출력 함수 호출
				writeScriptfv1();

				//다음 출력 좌표 설정
				nYCurPos=nYCurPos+nBandHeight;
			}   //end if(bIsPrint)

			//bIsFirst 및 출력 라인 설정
			bIsFirst=false;
		}   //while end

		if(bIsFirst){   //while 루프가 실행되지 않은 경우
		} else {   //while 루프가 실행된 경우
			//루프 외부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_1(bIsFirst, true, true, true);   //fv_1 조건 밴드 호출
		}   //루프 외부 end if(bIsFirst)
	}

