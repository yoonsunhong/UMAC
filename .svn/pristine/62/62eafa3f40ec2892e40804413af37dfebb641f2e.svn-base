	//-----ResultSet, PreparedStatement, Connection, CallableStatement Close 함수-----
	private void cleanup(Object rs, PreparedStatement st, CallableStatement cs, Connection conn){
		if(rs != null){
			try{
				if(rs instanceof ResultSet){
					((ResultSet)rs).close();
				}else if(rs instanceof com.activeintra.util.uds.AIDataSource){
					((com.activeintra.util.uds.AIDataSource)rs).close();
				}else if(rs instanceof com.activeintra.util.uds.XMLSetHandler){
					((com.activeintra.util.uds.XMLSetHandler)rs).close();
				}else if(rs instanceof com.activeintra.util.uds.CSVSetHandler){
					((com.activeintra.util.uds.CSVSetHandler)rs).close();
				}else if(rs instanceof com.activeintra.util.uds.JSONSetHandler){
					((com.activeintra.util.uds.JSONSetHandler)rs).close();
				}
			}
			catch(SQLException e){
				logger.error(e);
			}
		}

		if(st != null){
			try{
				st.close();
			}
			catch(SQLException e){
				logger.error(e);
			}
		}

		if(cs != null){
			try{
				cs.close();
			}
			catch(SQLException e){
				logger.error(e);
			}
		}

		if(conn != null){
			try{
				conn.close();
			}
			catch(SQLException e){
				logger.error(e);
			}
		}
	}

	//-----SQL문 변수 초기화 함수-----
	private void initSQLConst() throws Exception {
		sql_CUST_RECEIABLES.setLength(0);
		sql_CUST_RECEIABLES.append("            SELECT A.* ");
		sql_CUST_RECEIABLES.append("            ,      NVL(A.CREDIT_MONTH_AMT,0) + NVL(A.CREDIT_SALE,0) - NVL(A.DPOT_AMT,0) AS ACCT_REABLE ");
		sql_CUST_RECEIABLES.append("            ,      CASE WHEN A.CREDIT_LIMIT = 0 ");
		sql_CUST_RECEIABLES.append("                        THEN 0 ");
		sql_CUST_RECEIABLES.append("                        ELSE A.CREDIT_LIMIT - (A.CREDIT_MONTH_AMT + A.CREDIT_SALE - A.DPOT_AMT) ");
		sql_CUST_RECEIABLES.append("                   END AS ZAN_CREDIT_LIMIT ");
		sql_CUST_RECEIABLES.append("            ,      CASE ");
		sql_CUST_RECEIABLES.append("                   WHEN   LENGTH(A.MOBIL_NO_1)=10 ");
		sql_CUST_RECEIABLES.append("                   THEN   SUBSTR(A.MOBIL_NO_1,0,3)||'-'|| ");
		sql_CUST_RECEIABLES.append("                          SUBSTR(A.MOBIL_NO_1,4,3)||'-'|| ");
		sql_CUST_RECEIABLES.append("                          SUBSTR(A.MOBIL_NO_1,7,4) ");
		sql_CUST_RECEIABLES.append("                   WHEN   LENGTH(A.MOBIL_NO_1)=11 ");
		sql_CUST_RECEIABLES.append("                   THEN   SUBSTR(A.MOBIL_NO_1,0,3)||'-'|| ");
		sql_CUST_RECEIABLES.append("                          SUBSTR(A.MOBIL_NO_1,4,4)||'-'|| ");
		sql_CUST_RECEIABLES.append("                          SUBSTR(A.MOBIL_NO_1,8,4) ");
		sql_CUST_RECEIABLES.append("                   ELSE   A.MOBIL_NO_1 ");
		sql_CUST_RECEIABLES.append("                   END    AS MOBIL_NO ");
		sql_CUST_RECEIABLES.append("    ,   CASE WHEN A.LAST_SALE_DT IS NOT NULL THEN ");
		sql_CUST_RECEIABLES.append("         SUBSTR(A.LAST_SALE_DT,0,4)||'-'|| ");
		sql_CUST_RECEIABLES.append("        SUBSTR(A.LAST_SALE_DT,5,2)||'-'|| ");
		sql_CUST_RECEIABLES.append("        SUBSTR(A.LAST_SALE_DT,7,2) END AS LAST_SALE_DT_ ");
		sql_CUST_RECEIABLES.append("    ,   CASE WHEN A.LAST_RCP_DT IS NOT NULL THEN SUBSTR(A.LAST_RCP_DT,0,4)||'-'|| ");
		sql_CUST_RECEIABLES.append("        SUBSTR(A.LAST_RCP_DT,5,2)||'-'|| ");
		sql_CUST_RECEIABLES.append("        SUBSTR(A.LAST_RCP_DT,7,2) END AS LAST_RCP_DT_ ");
		sql_CUST_RECEIABLES.append("            FROM ");
		sql_CUST_RECEIABLES.append("                ( ");
		sql_CUST_RECEIABLES.append("                    SELECT  CDCL.CORP_CODE ");
		sql_CUST_RECEIABLES.append("                    ,       CDCL.CUST_NO ");
		sql_CUST_RECEIABLES.append("                    ,       CDCL.STR_CODE ");
		sql_CUST_RECEIABLES.append("                    ,       CC.CUST_NAME ");
		sql_CUST_RECEIABLES.append("                    ,       CC.ACCT_DEPT ");
		sql_CUST_RECEIABLES.append("                    ,       FN_GET_COMMON_CODE_NM('BUSI_FLAG',CC.BUSI_FLAG,CDCL.CORP_CODE) AS BUSI_FLAG_NAME ");
		sql_CUST_RECEIABLES.append("                    ,       FN_GET_COMMON_CODE_NM('MBR_GRADE',CC.MBR_GRADE,CDCL.CORP_CODE) AS MBR_GRADE_NAME ");
		sql_CUST_RECEIABLES.append("                    ,       NVL(TSALE.SALE_AMT,0)  AS CREDIT_SALE ");
		sql_CUST_RECEIABLES.append("                    ,       NVL(TDPOT.DPOT_AMT,0) - NVL(TOVER.PREPAY_SETT,0)  AS DPOT_AMT ");
		sql_CUST_RECEIABLES.append(" ");
		sql_CUST_RECEIABLES.append(" ");
		sql_CUST_RECEIABLES.append("                    ,      CASE WHEN REPLACE('^P_SALE_ED^','-','')  = REPLACE('^P_SALE_SD^','-','')  AND  REPLACE('^P_SALE_ED^','-','') = TO_CHAR(SYSDATE,'YYYYMMDD') ");
		sql_CUST_RECEIABLES.append("                           THEN ");
		sql_CUST_RECEIABLES.append(" ");
		sql_CUST_RECEIABLES.append("                                NVL(PRE.CREDIT_SALE,0) - NVL(TDPOT1.DPOT_TODAY_AMT,0) ");
		sql_CUST_RECEIABLES.append("                           ELSE ");
		sql_CUST_RECEIABLES.append(" ");
		sql_CUST_RECEIABLES.append("                                NVL(PRE.CREDIT_SALE,0) ");
		sql_CUST_RECEIABLES.append(" ");
		sql_CUST_RECEIABLES.append("                           END AS CREDIT_MONTH_AMT ");
		sql_CUST_RECEIABLES.append(" ");
		sql_CUST_RECEIABLES.append("                    ,       PKG_ENCRYPTION_AES.DEC_AES(CC.MOBIL_NO) AS MOBIL_NO_1 ");
		sql_CUST_RECEIABLES.append("                    ,       CC.CREDIT_LIMIT ");
		sql_CUST_RECEIABLES.append("                    ,       CDCL.LAST_SALE_DT ");
		sql_CUST_RECEIABLES.append("                    ,       CDCL.LAST_RCP_DT ");
		sql_CUST_RECEIABLES.append("                    FROM    CS_CREDIT_LEDGER  CDCL ");
		sql_CUST_RECEIABLES.append("                    ,       CS_CUST             CC ");
		sql_CUST_RECEIABLES.append("                    ,       ( SELECT ");
		sql_CUST_RECEIABLES.append("                                     CUST_NO ");
		sql_CUST_RECEIABLES.append("                              ,      SUM(NVL(CREDIT_SALE,0) - ( NVL(DPOT_AMT,0) - NVL(PREPAY_SETT,0) )) AS CREDIT_SALE ");
		sql_CUST_RECEIABLES.append("                              FROM   CS_DT_CREDIT_LEDGER C ");
		sql_CUST_RECEIABLES.append("                              WHERE  CORP_CODE  = '^P_CORP_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    STR_CODE   = '^P_STR_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    CUST_NO    = NVL('^P_CUST_NO^', CUST_NO) ");

		String p_SALE_SD = P_SALE_SD_Param.substring(6);
		if(p_SALE_SD.equals("01")){
			sql_CUST_RECEIABLES.append("                              AND    CREDIT_DT  BETWEEN SUBSTR(REPLACE('^P_SALE_SD^','-',''),1,6) || '00' AND SUBSTR(REPLACE('^P_SALE_SD^','-',''),1,6)||'00' ");
		}else{
			sql_CUST_RECEIABLES.append("                              AND    CREDIT_DT  BETWEEN SUBSTR(REPLACE('^P_SALE_SD^','-',''),1,6) || '00' AND TO_CHAR(TO_DATE(REPLACE('^P_SALE_SD^','-',''),'YYYYMMDD') - 1,'YYYYMMDD') ");
		}


		sql_CUST_RECEIABLES.append("                              GROUP  BY CUST_NO ");
		sql_CUST_RECEIABLES.append("                            ) PRE ");
		sql_CUST_RECEIABLES.append("                    ,       ( SELECT CUST_NO ");
		sql_CUST_RECEIABLES.append("                              ,      SUM(NVL(PAY_AMT,0) - NVL(ZAN_AMT,0)) AS SALE_AMT ");
		sql_CUST_RECEIABLES.append("                              FROM   SL_SALE_PAYMENT SP ");
		sql_CUST_RECEIABLES.append("                              WHERE  CORP_CODE = '^P_CORP_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    STR_CODE = '^P_STR_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    SALE_DT BETWEEN '^P_SALE_SD^' AND '^P_SALE_ED^' ");
		sql_CUST_RECEIABLES.append("                              AND    CANC_FLAG IN ('0','2') ");
		sql_CUST_RECEIABLES.append("                              AND    GRE_TYPE IN ('00','35') ");
		sql_CUST_RECEIABLES.append("                              AND    PAY_METH = '04' ");
		sql_CUST_RECEIABLES.append("                              GROUP  BY CUST_NO ");
		sql_CUST_RECEIABLES.append("                            ) TSALE ");
		sql_CUST_RECEIABLES.append("                    ,       ( SELECT CUST_NO ");
		sql_CUST_RECEIABLES.append("                              ,      SUM(NVL(DPOT_AMT,0)) AS DPOT_AMT ");
		sql_CUST_RECEIABLES.append("                              FROM   CS_LEDGER_SLIP_DTL CL ");
		sql_CUST_RECEIABLES.append("                              WHERE  CORP_CODE = '^P_CORP_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    DPOT_STR_CODE = '^P_STR_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    RCP_DT BETWEEN '^P_SALE_SD^' AND '^P_SALE_ED^' ");
		sql_CUST_RECEIABLES.append("                              GROUP  BY CUST_NO ");
		sql_CUST_RECEIABLES.append("                            ) TDPOT ");
		sql_CUST_RECEIABLES.append("                    ,       ( SELECT CUST_NO ");
		sql_CUST_RECEIABLES.append("                              ,      SUM(NVL(PREPAY_SETT,0)) AS PREPAY_SETT ");
		sql_CUST_RECEIABLES.append("                              FROM   CS_DPOT_OVER ");
		sql_CUST_RECEIABLES.append("                              WHERE  CORP_CODE = '^P_CORP_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    STR_CODE = '^P_STR_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    RCP_DT BETWEEN '^P_SALE_SD^' AND '^P_SALE_ED^' ");
		sql_CUST_RECEIABLES.append("                              GROUP  BY CUST_NO ");
		sql_CUST_RECEIABLES.append("                            ) TOVER ");
		sql_CUST_RECEIABLES.append("                     ,      (  SELECT CUST_NO ");
		sql_CUST_RECEIABLES.append("                              ,      SUM(NVL(DPOT_AMT,0)) AS DPOT_TODAY_AMT ");
		sql_CUST_RECEIABLES.append("                              FROM   CS_LEDGER_SLIP_DTL CL ");
		sql_CUST_RECEIABLES.append("                              WHERE  CORP_CODE = '^P_CORP_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    DPOT_STR_CODE = '^P_STR_CODE^' ");
		sql_CUST_RECEIABLES.append("                              AND    RCP_DT <  TO_CHAR(IDATE,'YYYYMMDD') ");
