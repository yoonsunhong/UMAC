		sql_CUST_RECEIABLES.append("                    AND     TOVER.CUST_NO(+)  = CDCL.CUST_NO ");
		sql_CUST_RECEIABLES.append("                    AND     TDPOT1.CUST_NO(+)  = CDCL.CUST_NO ");
		sql_CUST_RECEIABLES.append(" ");
