			if(sheet20_B2_Buf==null){
				sheet20_B2_Buf = sheet20_B2;
			}else{
				sheet20_B2_Buf=null;
			}
			sheet20_B2_Flag=true;
			writeScriptVirtualMergefv1();
		}
	}

