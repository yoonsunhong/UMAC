	sql_time.append(",	(SELECT TUI.USER_NM FROM TBL_USER_INFO TUI WHERE TUI.USER_ID='^P_EMP_NO^') AS USER_NM ");
	sql_time.append("FROM 	DUAL ");
	initializer.prepareQuery("sql_time", sql_time.toString());
}

