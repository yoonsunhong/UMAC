	//-----fv1 RepeatBand Start-----
	//-----소속 쉬트 : SHEET=Sheet2   FRAME=Frame0
	//-----밴드 이름 : fv1
	//-----밴드 종류 : 반복 밴드
	//-----bIsPrint : 감추기 속성 설정 여부 true면 출력, false면 스크립트를 생성하지 않음
	private void getScriptFromfv1(boolean bIsPrint) throws Exception{
		//지역 변수 선언 및 초기화
		int nBandHeight=15;    //밴드 출력 높이
		boolean bIsFirst=true;
		boolean bIsTrue_fv_2=false;   //fv_2 조건 밴드 플래그 선언
		boolean bIsTrue_fv_1=false;   //fv_1 조건 밴드 플래그 선언

		//데이터 변수 초기화
		sheet20_A2_Buf=null;
		sheet20_A2_Flag=true;
		sheet20_A2="";
		sheet20_B2_Buf=null;
		sheet20_B2_Flag=true;
		sheet20_B2="";
		sheet20_C2="";
		sheet20_D2="";
		sheet20_E2=0L;
		sheet20_F2=0L;
		sheet20_G2=0L;
		sheet20_H2=0;
		sheet20_I2=0;
		sheet20_J2=0;
		sheet20_K2=0;
		sheet20_L2=0L;
		sheet20_M2="";
		bEnabledVirtualMerge_fv1=false;

		//DataSet 객체 생성
		//ResultSet supply_sale 객체의 생성
		if(supply_sale==null){
			supply_sale = stat_supply_sale.executeQuery();
		}else{
			supply_sale.beforeFirst();
		}

		//--while Loop Start--##
		while(supply_sale.next()){   //자동으로 생성되는 반복 밴드의 루프문
			//데이터 fetch
			setFieldVariableFromsupply_sale();	//RecordSet supply_sale에서 필드 값 설정

			//조건 밴드 출력 조건문
			bIsBeforePageSkip=false;
			if(!supply_sale_VEN_NAME.equals(sheet20_B2)){    //fv_2 밴드 조건문
				bIsTrue_fv_2=true;
			}else{
				bIsTrue_fv_2=false;
			}

			if(bIsFirst){   //밴드 호출 후 루프 내에서 한번 만 실행되는 코드
				//조건 플래그 초기화
				bIsTrue_fv_2=false;    //fv_2 조건 밴드 플래그 초기화
				bIsTrue_fv_1=false;    //fv_1 조건 밴드 플래그 초기화
			}   //end if(bIsFirst)

			//데이터 변수 할당
			if(!sheet20_A2.equals(supply_sale_VEN_CODE)){
				if(!bIsFirst){
					sheet20_A2_Buf = sheet20_A2;
					sheet20_A2_Flag=true;
				}
				sheet20_A2=supply_sale_VEN_CODE;
			}
			if(!sheet20_B2.equals(supply_sale_VEN_NAME)){
				if(!bIsFirst){
					sheet20_B2_Buf = sheet20_B2;
					sheet20_B2_Flag=true;
				}
				sheet20_B2=supply_sale_VEN_NAME;
			}
			sheet20_C2 = supply_sale_SCAN_CODE;
			sheet20_D2 = supply_sale_ITM_NAME;
			sheet20_E2 = (long)supply_sale_SALE_QTY;
			sheet20_F2 = supply_sale_SALE_WPRC;
			sheet20_G2 = supply_sale_SALE_SAMT;
			sheet20_H2 = supply_sale_BOT_AMT;
			sheet20_I2 = supply_sale_DC_AMT;
			sheet20_J2 = supply_sale_MBR_DC_AMT;
			sheet20_K2 = (double)supply_sale_SALE_SPRC;
			sheet20_L2 = supply_sale_PROPIT_AMT;
			sheet20_M2 = supply_sale_PROPIT_RT;

			//루프 내부 가상병합 출력 함수 호출
			if(bIsPrint){
				writeScriptVirtualMergefv1();
			}

			//루프 내부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_2(bIsFirst, false, bIsTrue_fv_2, true);   //fv_2 조건 밴드 호출
			getScriptFromfv_1(bIsFirst, false, bIsTrue_fv_1, true);   //fv_1 조건 밴드 호출

			//AI 스크립트 출력
			if(bIsPrint){   //감추기 속성이 지정되지 않으면 bIsPrint는 true
				if(isPageSkip(nYCurPos+nBandHeight)){   //스크립트 출력 영역 검사
					//가상병합 출력 및 한번만 데이터 Reset
					pageSkipVirtualMergefv1();

					//페이지 스킵 함수 호출
					pageSkip(10);
				}   //end if(page skip test)

				//스크립트 출력 함수 호출
				writeScriptfv1();

				//페이지 연결 출력 및 가상병합 플래그 Reset
				if(sheet20_A2_Flag){
					sheet20_A2_YPos=nYCurPos;
					sheet20_A2_Buf=null;
					sheet20_A2_Flag=false;
				}
				if(sheet20_B2_Flag){
					sheet20_B2_YPos=nYCurPos;
					sheet20_B2_Buf=null;
					sheet20_B2_Flag=false;
				}
				bEnabledVirtualMerge_fv1=true;

				//다음 출력 좌표 설정
				nYCurPos=nYCurPos+nBandHeight;
			}   //end if(bIsPrint)

			//bIsFirst 및 출력 라인 설정
			bIsFirst=false;
		}   //while end

		if(bIsFirst){   //while 루프가 실행되지 않은 경우
		} else {   //while 루프가 실행된 경우
			//루프 외부 가상병합 출력 함수 호출
			sheet20_A2_Buf = sheet20_A2;
			sheet20_B2_Buf = sheet20_B2;
			if(bIsPrint){
				writeScriptVirtualMergefv1();
			}

			//루프 외부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_2(bIsFirst, true, true, true);   //fv_2 조건 밴드 호출
			getScriptFromfv_1(bIsFirst, true, true, true);   //fv_1 조건 밴드 호출
		}   //루프 외부 end if(bIsFirst)
	}

	//----fv1 반복밴드 Page Skip시 가상 병합 데이터 설정 및 출력 함수----
	private void pageSkipVirtualMergefv1(  ) throws Exception{
		//가상 병합 데이터 설정 및 출력
		if(bEnabledVirtualMerge_fv1){
			if(sheet20_A2_Buf==null){
				sheet20_A2_Buf = sheet20_A2;
			}else{
				sheet20_A2_Buf=null;
