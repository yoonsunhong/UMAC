public void jspInit() {
	initializer = new ReportInitializer(ResultSet.TYPE_SCROLL_INSENSITIVE, ResultSet.CONCUR_READ_ONLY);
	//수정되지 않는 정적 Query변수들을 초기화
	//sql_supply_sale
	StringBuilder sql_supply_sale = new StringBuilder();
	sql_supply_sale.setLength(0);
	sql_supply_sale.append("            SELECT  A.VEN_CODE ");
	sql_supply_sale.append("            ,       A.VEN_NAME ");
	sql_supply_sale.append("            ,       A.SCAN_CODE ");
	sql_supply_sale.append("            ,       A.ITM_NAME ");
	sql_supply_sale.append("            ,       A.SALE_QTY ");
	sql_supply_sale.append("            ,       A.SPRC ");
	sql_supply_sale.append("            ,       A.WPRC ");
	sql_supply_sale.append("            ,       A.BOT_AMT ");
	sql_supply_sale.append("            ,       A.DC_AMT ");
	sql_supply_sale.append("            ,       A.MBR_DC_AMT ");
	sql_supply_sale.append("            ,       (NVL(A.SPRC,0)*NVL(A.SALE_QTY,0)) - A.BOT_AMT - A.DC_AMT - A.MBR_DC_AMT AS SALE_SPRC ");
	sql_supply_sale.append("            ,       NVL(A.WPRC,0)*NVL(A.SALE_QTY,0) AS SALE_WPRC ");
	sql_supply_sale.append("            ,       NVL(A.SPRC,0)*NVL(A.SALE_QTY,0) AS SALE_SAMT ");
	sql_supply_sale.append("            ,       NVL(A.SALE_QTY,0) * NVL(A.SPRC,0)-NVL(A.SALE_QTY,0) * NVL(A.WPRC,0) - A.BOT_AMT - A.DC_AMT - A.MBR_DC_AMT AS PROPIT_AMT ");
	sql_supply_sale.append("            ,       DECODE(NVL(A.SALE_QTY,0) * NVL(A.SPRC,0),0,0 ");
	sql_supply_sale.append("            ,       TRUNC((NVL(A.SALE_QTY,0) * NVL(A.SPRC,0)-NVL(A.SALE_QTY,0) * NVL(A.WPRC,0)- A.BOT_AMT - A.DC_AMT - A.MBR_DC_AMT) * 100 /  (NVL(A.SALE_QTY,0) * NVL(A.SPRC,0)- - A.BOT_AMT - A.DC_AMT - A.MBR_DC_AMT),2)) AS PROPIT_RT ");
	sql_supply_sale.append("            FROM ");
	sql_supply_sale.append("            (SELECT ");
	sql_supply_sale.append("                    CS.VEN_CODE ");
	sql_supply_sale.append("            ,       CS.VEN_NAME ");
	sql_supply_sale.append("            ,       SSI.SCAN_CODE ");
	sql_supply_sale.append("            ,       CPC.ITM_NAME ");
	sql_supply_sale.append("            ,       SUM(NVL(SSI.SALE_QTY,0)) AS SALE_QTY ");
	sql_supply_sale.append("            ,       ROUND(SSI.SALE_PRC,0) AS SPRC ");
	sql_supply_sale.append("            ,       ROUND(CPS.WPRC,0) AS WPRC ");
	sql_supply_sale.append("            ,       SUM(NVL(SSI.BOT_AMT,0)) AS BOT_AMT ");
	sql_supply_sale.append("            ,       SUM(NVL(SSI.DC_AMT,0))+SUM(NVL(SSI.SPECIAL_DC_AMT,0)) AS DC_AMT ");
	sql_supply_sale.append("            ,       SUM(NVL(SSI.MBR_DC_AMT,0)) AS MBR_DC_AMT ");
	sql_supply_sale.append("            FROM    CD_SUPPLY CS ");
	sql_supply_sale.append("            ,       CD_PRODUCT_CMN CPC ");
	sql_supply_sale.append("            ,       CD_PRODUCT_STORE CPS ");
	sql_supply_sale.append("            ,       SL_SALE_HDR SSH ");
	sql_supply_sale.append("            ,       SL_SALE_ITEM SSI ");
	sql_supply_sale.append("            WHERE   CS.CORP_CODE    = CPC.CORP_CODE ");
	sql_supply_sale.append("            AND     CS.VEN_CODE     = CPC.VEN_CODE ");
	sql_supply_sale.append("            AND     CPC.CORP_CODE   = CPS.CORP_CODE ");
	sql_supply_sale.append("            AND     CPC.SCAN_CODE   = CPS.SCAN_CODE ");
	sql_supply_sale.append("            AND     CPC.ITM_CODE    = CPS.ITM_CODE ");
	sql_supply_sale.append("            AND     CPS.CORP_CODE   = SSH.CORP_CODE ");
	sql_supply_sale.append("            AND     CPS.STR_CODE    = SSH.STR_CODE ");
	sql_supply_sale.append("            AND     CPS.ITM_CODE    = SSI.ITM_CODE ");
	sql_supply_sale.append("            AND     CPS.SCAN_CODE   = SSI.SCAN_CODE ");
	sql_supply_sale.append("            AND     SSH.CORP_CODE   = SSI.CORP_CODE ");
	sql_supply_sale.append("            AND     SSH.STR_CODE    = SSI.STR_CODE ");
	sql_supply_sale.append("            AND     SSH.SALE_DT     = SSI.SALE_DT ");
	sql_supply_sale.append("            AND     SSH.POS_NO      = SSI.POS_NO ");
	sql_supply_sale.append("            AND     SSH.TRXN_NO     = SSI.TRXN_NO ");
	sql_supply_sale.append("            AND     SSH.CANC_FLAG IN  ('0','2') ");
	sql_supply_sale.append("            AND     SSH.GRE_TYPE    ='00' ");
	sql_supply_sale.append("            AND     SSH.STR_CODE    LIKE '%' || NVL('^P_STR_CODE^',SSH.STR_CODE)    || '%' ");
	sql_supply_sale.append("            AND     SSH.CORP_CODE   = '^P_CORP_CODE^' ");
	sql_supply_sale.append("            AND     SSH.SALE_DT  BETWEEN '^P_SALES_SD^' AND '^P_SALES_ED^' ");
	sql_supply_sale.append("            AND     CS.VEN_CODE     LIKE '%' ||NVL('^P_VEN_CODE^',CS.VEN_CODE)  || '%' ");
	sql_supply_sale.append("            AND     CS.VEN_NAME     LIKE '%' ||  NVL('^P_VEN_NAME^',CS.VEN_NAME) || '%' ");
	sql_supply_sale.append("            GROUP BY CS.VEN_CODE ");
	sql_supply_sale.append("            ,        CS.VEN_NAME ");
	sql_supply_sale.append("            ,        SSI.SCAN_CODE ");
	sql_supply_sale.append("            ,        CPC.ITM_NAME ");
	sql_supply_sale.append("            ,        SSI.SALE_PRC ");
	sql_supply_sale.append("            ,        CPS.WPRC ");
	sql_supply_sale.append("            ORDER BY CS.VEN_NAME ");
	sql_supply_sale.append("            ,        SSI.SCAN_CODE) A ");
	initializer.prepareQuery("sql_supply_sale", sql_supply_sale.toString());

	//sql_time
	StringBuilder sql_time = new StringBuilder();
	sql_time.setLength(0);
