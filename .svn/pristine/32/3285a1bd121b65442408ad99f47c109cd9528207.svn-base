				nYCurPos=nYCurPos+nBandHeight;
				sheet20_H3=0L;
			}
		}else{   //조건 밴드 출력 플래그 값이 false인 경우
		}   //end if(bIsTrue)

		if(!bIsLast){   //bIsLast가 false인 경우 - 조건 밴드의 수식 계산
			//데이터 변수 할당
			sheet20_H3=(long)SUM(sheet20_H2, sheet20_H3);
