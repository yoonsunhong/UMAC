		boolean bIsTrue_fv_sum=false;   //fv_sum 조건 밴드 플래그 선언

		//데이터 변수 초기화
		sheet20_A2="";
		sheet20_B2="";
		sheet20_C2="";
		sheet20_D2=0L;
