		//DataSet 객체 생성
		//ResultSet pickinglistDTL 객체의 생성
		if(pickinglistDTL==null){
			pickinglistDTL = stat_pickinglistDTL.executeQuery();
		}else{
			pickinglistDTL.beforeFirst();
		}

		//--while Loop Start--##
		while(pickinglistDTL.next()){   //자동으로 생성되는 반복 밴드의 루프문
			//데이터 fetch
			setFieldVariableFrompickinglistDTL();	//RecordSet pickinglistDTL에서 필드 값 설정

			if(bIsFirst){   //밴드 호출 후 루프 내에서 한번 만 실행되는 코드
				//조건 플래그 초기화
				bIsTrue_fv_sum=false;    //fv_sum 조건 밴드 플래그 초기화
			}   //end if(bIsFirst)

			//데이터 변수 할당
			sheet20_A2 = pickinglistDTL_SCAN_CODE;
			sheet20_B2 = pickinglistDTL_ITM_NAME;
			sheet20_C2 = pickinglistDTL_UNIT;
			sheet20_D2 = pickinglistDTL_IPSU_QTY;
			sheet20_E2 = pickinglistDTL_W_PUR_DT;
			sheet20_F2 = pickinglistDTL_W_VALID_END_DT;
			sheet20_G2 = pickinglistDTL_W_LINE_CODE;
			sheet20_H2 = pickinglistDTL_DOUT_QTY;

			//루프 내부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_sum(bIsFirst, false, bIsTrue_fv_sum, true);   //fv_sum 조건 밴드 호출

			//Auto Size Code
			nAutoHeight_fv1[0]=0;
			nAutoHeight_fv1[0]=getPrintHeight(sheet20_A2, charWidth, 10, 73, 17, nAutoHeight_fv1[0], 4, 1, "cell_style_5");
			nAutoHeight_fv1[0]=nAutoHeight_fv1[0]+18;
			nBandHeight=nAutoHeight_fv1[0];   //밴드 출력 높이 설정

			//셀 Rect, Style 값 저장 : getPrintHeight
			setCellInfoMap();

			//AI 스크립트 출력
			if(bIsPrint){   //감추기 속성이 지정되지 않으면 bIsPrint는 true
				if(isPageSkip(nYCurPos+nBandHeight)){   //스크립트 출력 영역 검사
					//페이지 스킵 함수 호출
					pageSkip(10);
				}   //end if(page skip test)

				//스크립트 출력 함수 호출
				writeScriptfv1();

				//다음 출력 좌표 설정
				nYCurPos=nYCurPos+nBandHeight;
			}   //end if(bIsPrint)

			//bIsFirst 및 출력 라인 설정
			bIsFirst=false;
		}   //while end

		if(bIsFirst){   //while 루프가 실행되지 않은 경우
		} else {   //while 루프가 실행된 경우
			//루프 외부 조건밴드 및 스타일 함수 호출
			getScriptFromfv_sum(bIsFirst, true, true, true);   //fv_sum 조건 밴드 호출
		}   //루프 외부 end if(bIsFirst)
	}

