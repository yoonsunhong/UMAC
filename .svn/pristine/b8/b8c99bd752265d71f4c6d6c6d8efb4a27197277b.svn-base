			}

			//데이터 fetch
			if(TIME.next()){
				setFieldVariableFromTIME();	//ResultSet TIME에서 필드 값 설정
			}else{
				setFieldInitFromTIME();	//ResultSet TIME의 필드 값 초기화
			}

			//데이터 변수 초기화
			sheet10_A2="";
