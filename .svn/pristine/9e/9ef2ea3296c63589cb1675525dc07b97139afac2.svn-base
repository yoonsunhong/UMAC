	sql_inout.append("        ,(SELECT ");
	sql_inout.append("          TB.STR_CODE ");
	sql_inout.append("        ,       NVL(SUM(TB.DOUT_TAX1_),0)  AS DOUT_TAX1_ ");
	sql_inout.append("        ,       NVL(SUM(TB.DOUT_TAX2_),0)   AS DOUT_TAX2_ ");
	sql_inout.append("        ,       NVL(SUM(TB.DOUT_TAX1_),0)+NVL(SUM(TB.DOUT_TAX2_),0) + NVL(SUM(TB.DOUT_TAX1_VAT_),0) AS DOUT_TAX_ ");
	sql_inout.append("        ,       NVL(SUM(TB.DOUT_TAX1_VAT_),0)   AS DOUT_TAX1_VAT_ ");
	sql_inout.append("        ,       NVL(SUM(TB.DIN_TAX1_),0)  AS DIN_TAX1_ ");
	sql_inout.append("        ,       NVL(SUM(TB.DIN_TAX2_),0)   AS DIN_TAX2_ ");
	sql_inout.append("        ,       NVL(SUM(TB.DIN_TAX1_),0)+NVL(SUM(TB.DIN_TAX2_),0) + NVL(SUM(TB.DIN_TAX1_VAT_),0) AS DIN_TAX_ ");
	sql_inout.append("        ,       NVL(SUM(TB.DIN_TAX1_VAT_),0)   AS DIN_TAX1_VAT_ ");
	sql_inout.append("        FROM( ");
	sql_inout.append("        SELECT ");
	sql_inout.append("        ID.* ");
	sql_inout.append("        ,       CASE WHEN IH.DIN_CFM_DT IS NULL THEN ");
	sql_inout.append("                     'N' ");
	sql_inout.append("                     WHEN IH.DIN_CFM_DT IS NOT NULL THEN ");
	sql_inout.append("                     'Y' ");
	sql_inout.append("                END AS  CFM_YN ");
	sql_inout.append("        , IH.DIN_STR_CODE ");
	sql_inout.append("        FROM PO_INOUT_CENTER_HDR IH ");
	sql_inout.append("        , ( ");
	sql_inout.append("        SELECT CORP_CODE ");
	sql_inout.append("        , STR_CODE ");
	sql_inout.append("        , SLIP_NO ");
	sql_inout.append("        , DOUT_DT ");
	sql_inout.append(" ");
	sql_inout.append("        					,		SUM(CASE    WHEN    TAX_GB='1'  THEN ");
	sql_inout.append("                               TRUNC(DOUT_CFM_QTY * (DOUT_WPRC + DOUT_WVAT))   - ROUND(DOUT_CFM_QTY * (DOUT_WPRC + DOUT_WVAT)/11, 2) ");
	sql_inout.append("                            END) AS DOUT_TAX1_ ");
	sql_inout.append("                    ,       SUM(CASE    WHEN    TAX_GB='2'  THEN ");
	sql_inout.append("                               TRUNC(DOUT_CFM_QTY * DOUT_WPRC) ");
	sql_inout.append("                            END) AS DOUT_TAX2_ ");
	sql_inout.append("                    ,       SUM(CASE    WHEN    TAX_GB='1'  THEN ");
	sql_inout.append("                               TRUNC(DIN_CFM_QTY * (DIN_WPRC + DIN_WVAT))   - ROUND(DIN_CFM_QTY * (DIN_WPRC + DIN_WVAT)/11, 2) ");
	sql_inout.append("                            END) AS DIN_TAX1_ ");
	sql_inout.append("                    ,       SUM(CASE    WHEN    TAX_GB='2'  THEN ");
	sql_inout.append("                               TRUNC(DIN_CFM_QTY * DIN_WPRC) ");
	sql_inout.append("                            END) AS DIN_TAX2_ ");
	sql_inout.append("                    ,       SUM(CASE    WHEN    TAX_GB='1'  THEN ");
	sql_inout.append("                                ROUND(DOUT_CFM_QTY * (DOUT_WPRC + DOUT_WVAT)/11,2) ");
	sql_inout.append("                            END) AS DOUT_TAX1_VAT_ ");
	sql_inout.append("                    ,       SUM(CASE    WHEN    TAX_GB='2'  THEN ");
	sql_inout.append("                                0 ");
	sql_inout.append("                            END) AS DOUT_TAX2_VAT_ ");
	sql_inout.append("                    ,       SUM(CASE    WHEN    TAX_GB='1'  THEN ");
	sql_inout.append("                                	ROUND(DIN_CFM_QTY * (DIN_WPRC + DIN_WVAT)/11,2) ");
	sql_inout.append("                            END) AS DIN_TAX1_VAT_ ");
	sql_inout.append("                    ,       SUM(CASE    WHEN    TAX_GB='2'  THEN ");
	sql_inout.append("                               0 ");
	sql_inout.append("                            END) AS DIN_TAX2_VAT_ ");
	sql_inout.append("        FROM PO_INOUT_CENTER_DTL ");
	sql_inout.append("        GROUP BY ");
	sql_inout.append("        CORP_CODE ");
	sql_inout.append("        , STR_CODE ");
	sql_inout.append("        , SLIP_NO ");
	sql_inout.append("        , DOUT_DT ");
	sql_inout.append("        , TAX_GB ");
	sql_inout.append("        ) ID ");
	sql_inout.append("        WHERE IH.CORP_CODE = ID.CORP_CODE ");
	sql_inout.append("        AND IH.STR_CODE = ID.STR_CODE ");
	sql_inout.append("        AND IH.SLIP_NO = ID.SLIP_NO ");
	sql_inout.append("        AND IH.DOUT_DT = ID.DOUT_DT ");
	sql_inout.append("        AND IH.CORP_CODE = '^P_CORP_CODE^' ");
	sql_inout.append(" ");
	sql_inout.append("        AND IH.DIN_STR_CODE = NVL('^P_STR_CODE^', IH.DIN_STR_CODE) ");
	sql_inout.append("        AND IH.DIN_CFM_DT BETWEEN '^P_DOUT_STR_DT^' AND '^P_DOUT_END_DT^' ");
	sql_inout.append("        ) TB ");
	sql_inout.append("        WHERE TB.CFM_YN = NVL('^P_CFM_YN^', TB.CFM_YN) ");
	sql_inout.append("        GROUP BY TB.STR_CODE) D ");
	sql_inout.append("        WHERE A.DIN_STR_CODE  = B.STR_CODE(+) ");
	sql_inout.append("        AND   A.DIN_STR_CODE  = C.DIN_STR_CODE(+) ");
	sql_inout.append("        AND   A.DIN_STR_CODE  = D.STR_CODE(+) ");
	sql_inout.append("        ORDER BY A.DIN_STR_CODE ");
	initializer.prepareQuery("sql_inout", sql_inout.toString());
}

