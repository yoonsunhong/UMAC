	private void getScriptFromH(boolean bIsFirst, boolean bIsPrint) throws Exception{
		if(bIsFirst){   //밴드 최초 호출 시에만 실행되는 코드
			//DataSet 객체 생성
			//ResultSet param 객체의 생성
			if(param==null){
				param = stat_param.executeQuery();
			}else{
				param.beforeFirst();
			}

			//데이터 fetch
			if(param.next()){
				setFieldVariableFromparam();	//ResultSet param에서 필드 값 설정
			}else{
				setFieldInitFromparam();	//ResultSet param의 필드 값 초기화
			}

			//데이터 변수 초기화
			AI_STRING_OBJ5="";

			//데이터 변수 할당
			AI_STRING_OBJ5 = param_T;
		}   //end if(bIsFirst)

		//AI 스크립트 출력
		if(bIsPrint){   //감추기 속성이 지정되지 않으면 bIsPrint는 true
			//스크립트 출력 함수 호출
			writeScriptH();
		}   //end if(bIsPrint)
	}

